{
  "name": "redux-router-multi-step",
  "version": "1.0.0",
  "description": "A simple example of multi step form with Redux and React Router",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --colors --progress",
    "prebuild": "npm run clean",
    "clean": "rimraf dist",
    "start": "cross-env NODE_ENV=development PORT=3001 node index --colors",
    "serve": "cross-env NODE_ENV=production PORT=8082 node index --colors",
    "preserve": "npm run build",
    "lint": "eslint src",
    "e2e": "cross-env NODE_ENV=test PORT=3000 mocha './src/**/*.e2e.js' --compilers js:babel-register --recursive --require ./test.setup.js",
    "test": "cross-env NODE_ENV=test mocha './src/**/*.spec.js' --compilers js:babel-register --recursive --require ./test.setup.js",
    "pretest": "npm run lint"
  },
  "author": "",
  "license": "ISC",
